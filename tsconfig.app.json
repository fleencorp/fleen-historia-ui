/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": []
  },
  "files": [
    "src/main.ts"
  ],
  "paths": {
    "@app/*": ["src/app/*"],
    "@shared/*": ["src/app/shared/*"],
    "@base/*": ["src/app/base/*"],
    "@model/*": ["src/app/model/*"],
    "@constant": ["src/app/constant"],
  },
  "include": [
    "src/**/*.d.ts"
  ]
}
